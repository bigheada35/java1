
public class TestEndRpt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		{
			System.out.println("---문제2---");
			
			/*
			 *  swithc 문에서 아래의 프로그래밍을 짜시오.
			 *  
			 * 1234 월 봄입니다.출력
				5678 월 여름입니다.출력
				9 10 11 12 월 겨울 입니다. 출력
			 */
			int month = 3;
			//int month = 13;
			switch(month) {
			case 1:
			case 2:
			case 3:
			case 4:
				System.out.println(month + " 월 봄입니다.");
				break;
			case 5:
			case 6:
			case 7:
			case 8:
				System.out.println(month + " 월 여름입니다.");
				break;
			case 9:
			case 10:
			case 11:
			case 12:
				System.out.println(month + "월 겨울 입니다.");
				break;
			default:
				System.out.println("오류");
				break;
			}
			
			
		}
		
		
		{
			System.out.println("---문제2---");
			/*
			 * int num = -10 을 할당 후 해당 정수에 대한 절댓값을 출력하는 프로그램을 작성하시오.
				절대값은 양수 입니다.
			 */
			int num = -10;
			//int num = 5;
			int abs=0;
			//System.out.println(num);
			if(num<0) {
				abs = -(num);
			}else {
				abs = num;
			}
			System.out.println(num + "의 절댓값:"+abs);
		}

		
		{
			System.out.println("---문제5---");
			/*
			 * 구구단 6단을 출력 하는 프로그램을 짜시오
			 */
			int dan = 6;
			int res = 0;
			for(int i=1; i<=9 ; i++){
				res = dan * i;
				System.out.println( dan + " * " + i +" = " + res);
			}
		}
		
		{
			System.out.println("---문제6---");
			/*
			 * 1부터 10000 까지의 합을 구하는 프로그램을 짜시오.
			 */
			int sum=0;
			for(int i=1; i<=10000; i++){
				sum += i;
			}
			System.out.println(" 1~10000 까지의 합: " + sum);
		}
		
		{
			System.out.println("---문제7---");
			/*
			 * 1부터 10000 까지의 숫자중 홀수 의 합을 구하는 프로그램을 짜시오.
			 */
			int sum_odd = 0;
			for(int i=1; i<=10000; i++){
				if(i%2 != 0){
					sum_odd += i;
				}
			}
			System.out.println("1~10000까지홀수의합:"+sum_odd);
			
		}
		
		{
			System.out.println("---문제9---");
			/*
			 * 1과 100 사이의 숫자중 5의 배수이자 7의 배수의 첫번재 수는?(프로그램밍 하시오)
			 */
			int i=0;
			for(i=1; i<=100; i++){
				if( (i%5==0) && (i%7==0)){
					break;
				}
			}
			System.out.println("1과 100 사이의 숫자중 5의 배수이자 7의 배수의 첫번재 수:"+i);
		}
		
		
		{
			System.out.println("---문제10---");
			/*
			 * 1과 10000 사이의 숫자중 5의 배수이자 7의 배수의  갯수는?(프로그램밍 하시오)
			 */
			int i=0;
			int count =0;
			for(i=1; i<=10000; i++){
				if((i%5 == 0) && (i%7 == 0)){
					count++;
				}
			}
			System.out.println("1과 10000 사이의 숫자중 5의 배수이자 7의 배수의 갯수:" +count);	
		}
		
		
		
	}

}
