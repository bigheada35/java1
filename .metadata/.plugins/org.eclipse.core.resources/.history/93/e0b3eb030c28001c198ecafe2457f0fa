
public class TodayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		{
			int mon = 126500;
			int mon_50000 = mon/50000;
			int mon_10000 = mon/10000;
			int mon_5000 = mon/5000;
			int mon_1000 = mon/1000;
			int mon_500 = mon/500;
			int mon_100 = mon/100;
			System.out.println("오만원:"+ mon_50000 +"장");
			System.out.println("만원:"+ mon_10000 +"장");
			System.out.println("오천원:"+ mon_5000 +"장");
			System.out.println("천원:"+ mon_1000 +"장");
			System.out.println("오백원:"+ mon_500 +"장");
			System.out.println("백원:"+ mon_100 +"장");
		}
	}

}
